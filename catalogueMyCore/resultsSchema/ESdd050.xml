                        <item xml:id="a10">
                           <locus target="#00" n="#"><span style="font-size: small;">&#160;<span lang="EN-US" unicode="" style="line-height: 115%;">Fol. 189vb: Record concerning loans or transactions (?), incomplete.</span></span></locus>
                           <q xml:lang="gez"/>
